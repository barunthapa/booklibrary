<h1>Categories#index</h1>
<p>Find me in app/views/categories/index.html.erb</p>
<ul>
	<% @categories.each do |book| %>
		<li> 
			<h3>
				<%= book.name %> </h3>
				<p> <%= link_to "read", category_path(book.id)%> </p>
					
	</li>

	<% end %> 
